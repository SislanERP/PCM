<% content_for :title, "PCM - Dashboard" %>
<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-9">
    <h2>Dashboard</h2>
    <ol class="breadcrumb">
      <%= render 'common/breadcrumb_home' %>
    </ol>
  </div>
</div><!-- END BreadCrumb -->

<div class="row">
  <div class="col-lg-12">
    <div class="wrapper wrapper-content animated fadeInRight">
      <div class="ibox">
        <div class="row">

          <div class="col-lg-6">
            <div class="ibox-title">
              <div class="m-b-md">
                <h4><%= t('branches') %></h4>
              </div>
            </div>
            <div class="ibox-content">
              <div class="row">
                <%= render 'branches/branch' %>
              </div> <!-- row -->
            </div> <!-- ibox-content -->
          </div> <!-- col-lg-6 BANCHES-->

          <div id="detail" class="col-lg-6">
          </div> <!-- col-lg-6 DETAIL-->

        </div> <!-- row -->
      </div> <!-- ibox -->
    </div> <!-- wrapper wrapper-content animated fadeInRight -->
  </div> <!-- col-lg-12 -->
</div> <!-- row -->
