<% @tasks.each do |task| %>

  <div class="timeline-item">
    <div class="row">
      <div class="col-xs-3 date">
        <i class="<%= task.task_type.icon %>"></i>
        <%= task.sequence %>
      </div>
      <div class="col-xs-9 content no-top-border">
        <p class="m-b-xs">
          <strong>
            <%= task.description %>
          </strong>
        </p>
      </div>
    </div>
  </div>

<% end %>
